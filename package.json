{
  "name": "conversationaliq",
  "version": "1.0.0",
  "description": "AI-powered real-time conversation intelligence platform for Zendesk",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "frontend:dev": "vite --config vite.config.ts",
    "frontend:build": "vite build --config vite.config.ts",
    "frontend:preview": "vite preview --config vite.config.ts",
    "build": "tsc --project tsconfig.backend.json && tsc-alias && npm run frontend:build",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit --project tsconfig.backend.json",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageThreshold.global.branches=100 --coverageThreshold.global.functions=100 --coverageThreshold.global.lines=100 --coverageThreshold.global.statements=100",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=unit-tests",
    "test:integration": "jest --testPathPattern=integration-tests",
    "test:performance": "jest --testPathPattern=performance-tests --runInBand --detectOpenHandles",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:security": "tsx src/testing/security-test-runner.ts",
    "test:comprehensive": "tsx src/testing/comprehensive-test-runner.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance && npm run test:security",
    "db:generate": "prisma generate",
    "db:migrate": "tsx scripts/migrate.ts migrate",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "tsx scripts/migrate.ts seed",
    "db:reset": "tsx scripts/migrate.ts reset",
    "db:deploy": "prisma migrate deploy",
    "kafka:dev": "docker-compose -f docker-compose.kafka.yml up -d",
    "kafka:stop": "docker-compose -f docker-compose.kafka.yml down",
    "kafka:logs": "docker-compose -f docker-compose.kafka.yml logs -f kafka",
    "services:up": "docker-compose -f docker-compose.kafka.yml up -d",
    "services:down": "docker-compose -f docker-compose.kafka.yml down",
    "pre-commit": "npm run lint && npm run type-check && npm run test && npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raosunjoy/ConversationalIQ.git"
  },
  "keywords": [
    "ai",
    "conversation-intelligence",
    "zendesk",
    "customer-service",
    "sentiment-analysis",
    "intent-prediction"
  ],
  "author": "ConversationIQ Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raosunjoy/ConversationalIQ/issues"
  },
  "homepage": "https://github.com/raosunjoy/ConversationalIQ#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@vitejs/plugin-react": "^4.1.1",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@apollo/server": "^4.12.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@graphql-tools/schema": "^10.0.25",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@prisma/client": "^5.3.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@zendeskgarden/react-buttons": "^8.69.5",
    "@zendeskgarden/react-chrome": "^8.69.5",
    "@zendeskgarden/react-forms": "^8.69.5",
    "@zendeskgarden/react-grid": "^8.69.5",
    "@zendeskgarden/react-loaders": "^8.69.5",
    "@zendeskgarden/react-notifications": "^8.69.5",
    "@zendeskgarden/react-tags": "^8.69.5",
    "@zendeskgarden/react-theming": "^8.69.5",
    "@zendeskgarden/react-tooltips": "^8.69.5",
    "@zendeskgarden/react-typography": "^8.69.5",
    "@types/ws": "^8.18.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.20",
    "graphql-ws": "^6.0.6",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "morgan": "^1.10.1",
    "prisma": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  }
}
