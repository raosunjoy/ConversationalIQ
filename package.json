{
  "name": "conversationaliq",
  "version": "1.0.0",
  "description": "AI-powered real-time conversation intelligence platform for Zendesk",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageThreshold.global.branches=100 --coverageThreshold.global.functions=100 --coverageThreshold.global.lines=100 --coverageThreshold.global.statements=100",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "pre-commit": "npm run lint && npm run type-check && npm run test && npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raosunjoy/ConversationalIQ.git"
  },
  "keywords": [
    "ai",
    "conversation-intelligence",
    "zendesk",
    "customer-service",
    "sentiment-analysis",
    "intent-prediction"
  ],
  "author": "ConversationIQ Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raosunjoy/ConversationalIQ/issues"
  },
  "homepage": "https://github.com/raosunjoy/ConversationalIQ#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "prisma": "^5.3.1"
  }
}
